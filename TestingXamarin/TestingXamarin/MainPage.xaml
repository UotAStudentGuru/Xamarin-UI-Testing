<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TestingXamarin"
             x:Class="TestingXamarin.MainPage">
    <StackLayout HorizontalOptions="Center">
        <Entry x:Name="entry" AutomationId="EntryId"/>
        <Button x:Name="button" Clicked="button_Clicked" AutomationId="ButtonID"/>
        <Label x:Name="Label" AutomationId="LabelID"/>
    </StackLayout>
	
</ContentPage>